                "p": {
                  "color": "@infoColor",
                  "line-height": "1.5"
                },
                ".display-4": {
                  "font-size": "16px"
                }
              }
            },
            ".card .panel-body p": {
              "color": "#24540b"
            },
            ".card .card-header a.panel-title h4": {
              "color": "#28303f"
            }
