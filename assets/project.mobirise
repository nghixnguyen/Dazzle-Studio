                    "height": "2px",
                    "margin-left": "0",
                    "width": "12px",
                    "border-radius": "50%",
                    "border": "2px solid rgba(97, 114, 138, 0.4)",
                    "padding": "13px",
                    "&:before, &:after": {
                      "background": "currentColor",
                      "border": "0",
                      "box-sizing": "border-box",
                      "content": "\"\"",
                      "display": "block",
                      "left": "50%",
