        {
          "_name": "accordion02",
          "_sourceTheme": "visionm4",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"accordion2\" group=\"Accordions\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-switch-arrow, mobirise-icons2\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMainTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showmainsubTitle\" checked>\n        <select title=\"Amount\" name=\"cardAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <header>Color</header>\n        <input type=\"color\" title=\"Subtitle BG\" name=\"subtitle\" value=\"#e7e7fb\" selected condition=\"showmainsubTitle\">\n        <input type=\"color\" title=\"Border\" name=\"border\" value=\"#1c1c25\" selected>\n        <input type=\"color\" title=\"Background\" name=\"cardbg\" value=\"#f8f8f8\" selected>\n        <input type=\"color\" title=\"Hover\" name=\"cardhover\" value=\"#c2f2ab\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/mbr-5-1920x1280.jpg\" selected>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\">\n            <input type=\"video\" title=\"Video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n\n            <div class=\"col-md-10 col-lg-7 col-12\">\n                <h4 class=\"mbr-section-title mbr-fonts-style align-center mb-3\" mbr-if=\"showMainTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\"><b>FOOT CARE SERVICES</b></h4>\n\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style align-center mb-5\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">For sanitation purposes, our pedicure chairs use single-use pedicure liners after each\n<div>and every client to further improve the spa pedicure experience.</div></h5>\n            </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"col-lg-6 col-md-12\">\n                <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n                    <div class=\"card mb-3\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Spa Pedicure</b><span class=\"price\">$40</span>\n                                    </h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        <div id=\"collapse1\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                    Whirlpool salt soak, cuticle and nail trimming, nail shaping, buffing, scrubbing,\n                                    foot massage followed by hot towel and polish.</p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card mb-3\" mbr-if=\"cardAmount > 1\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse2\" aria-expanded=\"false\" aria-controls=\"collapse2\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Spa Pedicure with Callus treatment</b> <span class=\"price\">$50</span></h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        <div id=\"collapse2\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                   Whirlpool salt soak, cuticle and nail trimming, nail shaping, buffing, scrubbing,\n                                    foot massage followed by hot towel and polish and callus treatment.\n                                </p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card mb-3\" mbr-if=\"cardAmount > 2\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse3\" aria-expanded=\"false\" aria-controls=\"collapse3\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Spa Pedicure with extended 10 minutes massage<span class=\"price\">$55</span></b>\n                                    </h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n\n                        <div id=\"collapse3\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                    Whirlpool salt soak, cuticle and nail trimming, nail shaping, buffing, scrubbing,\n                                    foot massage followed by hot towel and polish and callus treatment and enjoy 10 minutes massage.</p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card mb-3\" mbr-if=\"cardAmount > 3\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse4\" aria-expanded=\"false\" aria-controls=\"collapse4\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Paraffin Spa Pedicure</b><span class=\"price\">$50</span></h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        \n                        <div id=\"collapse4\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                    Enjoy a paraffin warm wax for dry skin. This pedicure helps to increase blood circulation and hydrates the skin, leaving feet soft and moisturized.\n                                </p>\n                            </div>\n                        </div>\n                       \n                    </div>\n                    \n                   \n                </div>\n            </div>\n\n\n            <div class=\"col-lg-6 col-md-12\">\n                <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n                    <div class=\"card mb-3\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne1\">\n\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse7\" aria-expanded=\"false\" aria-controls=\"collapse7\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Organic Spa Pedicure</b><span class=\"price\">$60</span>\n                                    </h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        <div id=\"collapse7\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne1\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                    All organic products. Toenails filed and shaped, cuticles treated, pumice scrub, sugar\nscrub, mask moisturizer, foot massage followed by hot towel and polish of your choice.</p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card mb-3\" mbr-if=\"cardAmount > 1\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne1\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse8\" aria-expanded=\"false\" aria-controls=\"collapse8\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Pearl Spa In The Box Pedicure</b><span class=\"price\">$65</span></h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        <div id=\"collapse8\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne1\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">A personal favorite of mine is “Spa in the Box”. It is a complete six steps pedicure\nexperience including a Pearl powder soak to help relieve dry skin. Hydrating Cleanser\nthoroughly cleanses with a rich and silky lather that washes away surface oils,\nperspiration, and debris. Sugar Scrub gently buffs away dry skin and surface cells to\nreveal fresher, more vibrant skin. Nourishing mask that smooths, softens, and\nde-stresses rough and tired feet. Massage Gel is an incredibly rich moisture treatment\nthat soothes and softens dry, rough, uncomfortable feet followed by moisturizing lotion\nfor softer feeling skin that retains moisture longer and polish of your choice. All the\nproducts used in this service is 100% vegan.</p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card mb-3\" mbr-if=\"cardAmount > 2\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne1\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse9\" aria-expanded=\"false\" aria-controls=\"collapse9\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Express pedicure</b><span class=\"price\">$30</span>\n                                    </h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        <div id=\"collapse9\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne1\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                    This is designed for an appointment between regular pedicures. Soaking, shaping, cuticle care and polish.\n                                </p>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card mb-3\" mbr-if=\"cardAmount > 3\">\n                        <div class=\"header-wrapper\">\n                            <div class=\"hover-bg\"></div>\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne1\">\n                                <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse10\" aria-expanded=\"false\" aria-controls=\"collapse10\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Polish change</b><span class=\"price\">$17</span></h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-down inactive\"></span>\n                                </a>\n                            </div>\n                        </div>\n                        <div id=\"collapse10\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne1\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                            <div class=\"panel-body\">\n                                <p class=\"mbr-fonts-style panel-text\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                    Apply a new polish of your choice.</p>\n                            </div>\n                        </div>\n                    </div>\n                    \n                    \n                </div>\n            </div>\n\n\n        </div>\n    </div>\n</section>",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-iconfont": {
              "font-size": "1.4rem !important",
              "font-family": "'Moririse2' !important",
              "color": "@primaryColor",
              "margin-left": "1rem"
            },
            ".main-sub-title": {
              "background": "@subtitle",
              "border-radius": "8px",
              "display": "inline-block",
              "width": "fit-content",
              "padding": "0.1rem 1rem"
            },
            ".panel-group": {
              "border": "none"
            },
            ".panel-title": {
              "display": "flex",
              "align-items": "center",
              "justify-content": "space-between"
            },
            ".panel-body": {
              "padding": "1rem 2rem"
            },
            ".header-wrapper": {
              "position": "relative"
            },
            ".card-header": {
              "background": "transparent",
              "border": "2px solid transparent",
              "position": "relative",
              "transition": "all 0.3s",
              "border-radius": "30px!important",
              "padding": "1rem 2rem",
              "z-index": "2"
            },
            "span": {
              "transition": "all 0.3s",
              "color": "@border !important"
            },
            ".card": {
              "overflow": "visible",
              "&:hover": {
                ".card-header": {
                  "border-color": "@border"
                },
                ".hover-bg": {
                  "background": "@cardhover",
                  "right": "-8px",
                  "top": "8px"
                },
                "span": {
                  "transform": "rotate(-90deg)"
                }
              }
            },
            ".hover-bg": {
              "position": "absolute",
              "right": "0px",
              "top": "0px",
              "width": "100%",
              "transition": "all 0.3s",
              "height": "100%",
              "background": "@cardbg",
              "border-radius": "30px!important",
              "z-index": "1"
            },
            "@media (max-width: 768px)": {
              ".panel-body": {
                "padding": "1rem 0rem"
              },
              ".card-header": {
                "padding": "1rem 1rem"
              }
            },
            "H4": {
              "color": "#777d74"
            },
            ".panel-title-edit": {
              "color": "#1c1c25"
            },
            ".mbr-section-title": {
              "color": "#78021a"
            },
            ".mbr-section-subtitle": {
              "color": "#040404"
            },
            ".main-sub-title, .align-wrapper": {
              "color": "#33358c"
            },
            ".price": {
              "padding": "1rem 1rem",
              "color": "#F70535",
              "font-weight": "bold"
            },
            ".mbr-section-subtitle DIV": {
              "text-align": "center"
            },
            ".panel-text": {
              "color": "#24540b"
            }
          },
          "_cid": "t2bzVObTPX",
          "_anchor": "accordion02-1l",
          "_PHPplaceholders": [],
          "_JSplaceholders": [],
          "_protectedParams": [],
          "_global": false,
          "_once": false,
          "_params": {}
        },
        {
          "_name": "accordion01",
          "_sourceTheme": "carm4",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"accordion1 carm4_accordion1\" group=\"Accordions\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax, 'mbr-fullscreen': fullScreen}\" plugins=\"mbr-switch-arrow2, mobirise-icons2\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"0\" condition=\"fullScreen == false\">\n        <select title=\"Accordions Count\" name=\"bootstrapAccordion\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\" selected>6</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showsubTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Column title\" name=\"showColumnTitle\">\n        <header condition=\"showTitle\">Mask</header>\n        <input type=\"checkbox\" title=\"Title Mask\" name=\"showTitleMask\" condition=\"showTitle\" checked>\n        <input type=\"color\" title=\"Mask Text Color\" name=\"maskTextColor\" value=\"#ffffff\" selected condition=\"showTitleMask && showTitle\">\n        <input type=\"checkbox\" title=\"Gradient Mask\" name=\"gradientMask\" condition=\"showTitleMask && showTitle\" checked>\n        <input type=\"color\" title=\"Mask Color\" name=\"maskColor\" value=\"#ff5e3a\" condition=\"showTitleMask && showTitle\">\n        <input type=\"color\" title=\"Mask Color 2\" name=\"maskColor2\" value=\"#ff653c\" condition=\"gradientMask && showTitleMask && showTitle\">\n        <input type=\"color\" title=\"Mask Color 3\" name=\"maskColor3\" value=\"#ff4e3c\" condition=\"gradientMask && showTitleMask && showTitle\">\n        <header>Color</header>\n        <input type=\"color\" name=\"cardColor\" title=\"Card Bg\" value=\"#f5f5f5\" selected>\n        <input type=\"color\" name=\"iconColor\" title=\"Icons\" value=\"#ff4e3c\" selected>\n        <input type=\"color\" name=\"lineColor\" title=\"Line\" value=\"#efefef\" selected>\n        <input type=\"color\" name=\"openLineColor\" title=\"Open Line\" value=\"#ff4e3c\" selected>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/mbr-6-1920x1280.jpg\" selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n            <input type=\"video\" title=\"Background Video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row\">\n            <div class=\"col-12 col-md-12 title-text\">\n                <h3 class=\"mbr-section-title mbr-bold align-center mbr-fonts-style\" data-app-selector=\".title-text .mbr-section-title\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">HAND CARE SERVICES</h3>\n                <h4 class=\"mbr-section-subtitle pb-1 align-center mbr-light mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showsubTitle\" data-app-selector=\".title-text .mbr-section-subtitle\"><b>All manicures include soaking, cuticle and nail trimming, nail shaping, hand massage</b><div><b></b>with polish of your choice.<br><b><br></b></div></h4>\n               \n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-lg-12\">\n                <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n                    <div class=\"row\">\n                        <div class=\"col-lg-6\">\n                            <h3 class=\"mbr-section-subtitle align-left mbr-black mbr-regular pb-0 pb-lg-4 mbr-fonts-style second-subtitle\" mbr-if=\"showColumnTitle\" mbr-theme-style=\"display-7\" data-app-selector=\".second-subtitle\">\n                                Personalization options</h3>\n                            <div class=\"card\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                    <a role=\"button\" class=\"panel-title collapsed text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n\n                                        <h4 class=\"mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\">\n                                            Paraffin Manicure <span class=\"special-treatment-price\">$30</span>\n                                            <span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                        </h4>\n\n                                    </a>\n                                </div>\n                                <div id=\"collapse1\" class=\"panel-collapse noScroll collapse \" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Enjoy a paraffin warm wax for dry skin. This manicure helps to increase blood circulation and hydrates the skin, leaving hands soft and moisturized.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 1\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingTwo\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse2\" aria-expanded=\"false\" aria-controls=\"collapse2\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Essie Gel Couture/ OPI Infinite Shine Manicure <span class=\"special-treatment-price\">$23</span>\n                                        </h4>\n                                    </a>\n\n                                </div>\n                                <div id=\"collapse2\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingTwo\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body pt\">\n                                            Shatterproof, gel-like results without a UV lamp. Quick drying top coat that seals and\nhelps protect, to magnify shine for an ultra-glossy flawless finish.\nRemove like regular nail polish.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 2\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingThree\">\n                                    <a role=\"button\" class=\"collapsed text-black panel-title pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse3\" aria-expanded=\"false\" aria-controls=\"collapse3\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Gel Manicure (LED) <span class=\"special-treatment-price\">$35</span>\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse3\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingThree\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">Gel is a nail product that can last up to 14 days without chipping, scratching or smudging.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 3\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFour\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse4\" aria-expanded=\"false\" aria-controls=\"collapse4\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                            Organic Gel Manicure <span class=\"special-treatment-price\">$42</span>\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse4\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFour\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            BSG is 5-free, with no formaldehyde, toluene, DBP, BHA or solvents (odourless,\ndustless and safe for all ages and even pregnant women) Absolutely no sanding,\ndrilling, primers or binders required. Gel color required LED cured.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 4\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFive\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse5\" aria-expanded=\"false\" aria-controls=\"collapse5\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                            Organic Manicure <span class=\"special-treatment-price\">$25</span>\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse5\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFive\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                           All Organic products. No UV/LED light.</p>\n                                    </div>\n                                </div>\n                            </div>\n                           \n                        </div>\n                        <div class=\"col-lg-6\">\n                            <h3 class=\"mbr-section-subtitle align-left mbr-black mbr-regular pb-0 pb-lg-4 mbr-fonts-style second-subtitle\" mbr-if=\"showColumnTitle\" mbr-theme-style=\"display-7\" data-app-selector=\".second-subtitle\">Social\n                                media integration</h3>\n                            <div class=\"card\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                    <a role=\"button\" class=\"panel-title collapsed text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse7\" aria-expanded=\"false\" aria-controls=\"collapse7\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Vegan Manicure <span class=\"special-treatment-price\">$27</span>\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse7\" class=\"panel-collapse noScroll collapse \" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            16+ Free Formula. Luxurious nail polishes that deliver gel-like wear and shine results\nyou expect with the clean formula you deserve. Hard-set nails in just 5 minutes. No UV\nlight.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 1\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingTwo\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse8\" aria-expanded=\"false\" aria-controls=\"collapse8\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Vegan Gel Manicure <span class=\"special-treatment-price\">$44</span>\n                                        </h4>\n                                    </a>\n\n                                </div>\n                                <div id=\"collapse8\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingTwo\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Vegan, Cruelty-free, Gluten-free\nNon-solvent, non-hazmat formula with no harmful fumes.\nLED Gel Color that provides vibrant, budge-proof and streak-free color.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 2\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingThree\">\n                                    <a role=\"button\" class=\"collapsed text-black panel-title pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse9\" aria-expanded=\"false\" aria-controls=\"collapse9\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Exfoliating</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse9\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingThree\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">Safely removes the outer layer of dead skin cells, and brings fresh, new skin to the\nsurface. It helps with anti -aging and hydration, and it lightens discoloration from sun\nspots.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 3\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFour\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse10\" aria-expanded=\"false\" aria-controls=\"collapse10\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                           Dip Powder/ Tips <span class=\"special-treatment-price\">$45+ / $60+</span>\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse10\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFour\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                           Provides 2+ weeks of superior strength and chip-free colour. Promotes natural nail\ngrowth. Applies onto natural nail or over artificial extensions. Air-dry with no LED/UV\nlamps needed. Odour-free. Soak-off removal.<br>z\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                          \n                            \n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
